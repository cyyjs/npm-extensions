{
    "name": "npm 搜索工具",
    "version": "0.0.1",
    "author": "cyy",
    "manifest_version": 2,
    "icons": {
        "16": "img/16.png",
        "32": "img/32.png",
        "48": "img/48.png",
        "128": "img/128.png"
    },
    "web_accessible_resources": ["img/128.png"],
    "permissions": ["tabs", "activeTab", "notifications", "https://www.npmjs.com/"],
    "description": "npm 搜索工具.",
    "browser_action": {
        "default_icon": "img/128.png",
        "default_title": "npm 搜索工具",
        "default_popup": "popup.html"
    }
}
